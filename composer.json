{
    "name": "pxgamer/justwatch",
    "type": "library",
    "description": "A PHP wrapper for the JustWatch API",
    "license": "MIT",
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "pxgamer\\JustWatch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pxgamer\\JustWatch\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
